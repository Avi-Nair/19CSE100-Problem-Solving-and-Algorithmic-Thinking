    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="limit, i, x, a" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter the number below which you want all primes&quot;" newline="True"/>
            <input variable="limit"/>
            <assign variable="i" expression="1"/>
            <while expression="i&lt;limit">
                <assign variable="a" expression="2"/>
                <assign variable="x" expression="0"/>
                <while expression="a&lt;i">
                    <if expression="i%a==0">
                        <then>
                            <assign variable="x" expression="x+1"/>
                        </then>
                        <else/>
                    </if>
                    <assign variable="a" expression="a+1"/>
                </while>
                <if expression="x==0">
                    <then>
                        <output expression="i" newline="True"/>
                    </then>
                    <else/>
                </if>
                <assign variable="i" expression="i+1"/>
            </while>
        </body>
    </function>
</flowgorithm>
